    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "exif": "^0.6.0",
    "express": "^4.16.3",
    "file-system": "^2.2.2",
    "graphql": "^14.0.2",
    "graphql-playground-middleware-express": "^1.7.3",
    "graphql-subscriptions": "^1.0.0",
