{
  "name": "portfolio-server",
  "version": "1.0.5",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon --exec babel-node src/index.js",
    "start": "babel-node src/index.js",
    "build": "NODE_ENV=production && rimraf dist && babel src -d dist",
    "build-webpack": "NODE_ENV=production && rimraf dist && webpack --config webpack.config.js",
