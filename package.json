{
  "name": "portfolio-server",
  "version": "1.3.1",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon --exec babel-node src/index.js",
    "start": "babel-node src/index.js",
    "build-babel": "NODE_ENV=production && rimraf dist && babel src -d dist",
    "build": "NODE_ENV=production && rimraf dist && webpack --mode production --config webpack.config.js",
    "build-dev": "NODE_ENV=development && rimraf dist && webpack --mode development --config webpack.config.js",
    "serve": "node dist/index.js",
    "prod": "npm run build && npm run serve",
    "test": "jest",
    "clear": "rm -rf uploads/* && rm -rf photos/*"
  },
  "author": "Mark Fairhurst",
  "license": "ISC",
  "dependencies": {
    "apollo-server": "^2.15.0",
    "apollo-server-express": "^2.15.0",
    "babel-preset-env": "^1.7.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "exif": "^0.6.0",
    "express": "^4.17.1",
    "file-system": "^2.2.2",
    "graphql": "^15.1.0",
    "graphql-playground-middleware-express": "^1.7.18",
    "graphql-subscriptions": "^1.1.0",
    "graphql-tools": "^6.0.10",
    "image-size": "^0.8.3",
    "jsonwebtoken": "^9.0.0",
    "merge-graphql-schemas": "^1.7.8",
    "mkdirp": "^1.0.4",
    "path": "^0.12.7",
    "pg": "^8.2.1",
    "progress-stream": "^2.0.0",
    "rimraf": "^3.0.2",
    "sequelize": "^5.21.13",
    "sharp": "^0.25.4",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^26.0.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-runtime": "^6.26.0",
    "copyfiles": "^2.3.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.21.2",
    "jest": "^26.0.1",
    "nodemon": "^2.0.4",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  }
}
